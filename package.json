{
  "name": "@bk62/buidl-protocol",
  "version": "1.0.0",
  "description": "Composable and decentralized social fundraising for beginning web3 developers",
  "scripts": {
    "compile": "hardhat compile",
    "spdx": "hardhat prepend-spdx-license",
    "test": "hardhat test ./test/unit/*.spec.ts --network hardhat",
    "test-staging": "hardhat test ./test/staging/*.spec.ts --network rinkeby",
    "lint": "solhint 'contracts/**/*.sol'",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "format": "prettier --write .",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js",
    "fuzzing": "docker run -it --rm -v $PWD:/src trailofbits/eth-security-toolbox"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bk62/build-protocol"
  },
  "bugs": {
    "url": "https://github.com/bk62/buidl-protocol/issues"
  },
  "homepage": "https://github.com/bk62/buidl-protocol#readme",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.10",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.1",
    "hardhat": "^2.9.0",
    "hardhat-contract-sizer": "^2.4.0",
    "hardhat-deploy": "^0.9.29",
    "hardhat-gas-reporter": "^1.0.7",
    "mocha": "^10.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.4.0",
    "typechain": "^7.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@appliedblockchain/chainlink-contracts": "0.0.4",
    "@appliedblockchain/chainlink-plugins-fund-link": "0.0.1",
    "@chainlink/contracts": "^0.4.0",
    "@chainlink/test-helpers": "^0.0.7-alpha",
    "@chainlink/token": "^1.1.0",
    "@openzeppelin/contracts": "^4.6.0",
    "@openzeppelin/contracts-upgradeable": "^4.6.0",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "babel-eslint": "^10.1.0",
    "dotenv": "^10.0.0"
  },
  "mocha": {
    "require": "ts-node/register/files",
    "timeout": 10000000
